    function f(){
    var x = input1.value
    var m = parseInt(x);
    alert(typeof m)
    var y = input1.value
    var n = parseInt(y);
    alert(typeof n)

    while (m != n && m != 0)
            {
                if (m > n)
                    m = m - n;
                else
                    n = n - m;
            }

            const tekst = "NWD to: ";
            p1.innerHTML = tekst + m
}
